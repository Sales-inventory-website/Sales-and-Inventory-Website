<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" type="text/css" href="/css/styly.css" />
	<title>Document</title>
</head>
<body>
<% include ./nav %>
<br>
<br>
<br>


<style>
h1{
	text-align: center;
	color: green;
	font-size: 20px
}

</style>
   

<h1> All Rim Reports</h1>
<table class="rtable">
    <thead>
      <tr>
        <th>Date</th>
        <th>Current Value</th>
        <th>Total Sales</th>
        <th>Total Cost</th>
        <th>Net Profit</th>
      </tr>
    </thead>

    <% datum.forEach(function(item) { %>
    <tbody>
      <tr>
        <td data-column="Date"><%= moment(item.date).format("LL") %></td>
        <td data-column="Current Value"><%= item.value %></td>
        <td data-column="Total Sales"><%= item.sales %></td>
        <td data-column="Total Cost"><%= item.cost %></td>
        <td data-column="Net Profit"><%= item.profit %></td>

      </tr>
    </tbody>
    <% }); %>  
  </table>

  
</body>
</html>