<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" type="text/css" href="/css/styly.css" />
	<title>Document</title>
</head>
<body>


<% include ./nav %>
<br>
<br>
<br>


<style>

h1{
	text-align: center;
	color: green;
	font-size: 20px
}

</style>
   
   <h1> All Total Report</h1>
   <table class="rtable">
      <thead>
        <tr>
          <th>Date</th>
          <th>Total Sales</th>
          <th>Total Profit</th>
          <th>Total Expenses</th>
          <th>Net Profit/Loss</th>
        </tr>
      </thead>

	  <% datas.forEach(function(item) { %>
	
	
      <tbody>
        <tr>
          <td data-column="Date"><%= moment(item.date).format("LL") %></td>
          <td id="tsales" data-column="Total Sales"><%= item.tsales %></td>
		  <td id="tprofit" data-column="Total Pofit"><%= item.tprofit %></td>
		  <td id="texpense" data-column="Total Expenses"><%= item.shop_expenses + item.good_transport + item.contribution + item.school_fees + item.coperative_contribution + item.others + item.household + item.salary  %></td>
          <td data-column="Net Profit/Loss"><%= item.net_profit %></td>
        </tr>
      </tbody>
      <% }); %>  
	</table>

		
</body>
</html>